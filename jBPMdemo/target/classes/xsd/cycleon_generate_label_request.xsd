<?xml version="1.0" encoding="UTF-8"?><xs:schema id="LabelRequest" elementFormDefault="qualified" targetNamespace="LabelRequest.xsd" xmlns="LabelRequest.xsd"		   xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:auth="AuthenticationCreateRequest.xsd" ><xs:import namespace="AuthenticationCreateRequest.xsd" schemaLocation="cycleon_create_authentication_request.xsd" />	<!-- UPDATES -->	<!-- Renamed PrintRequest to LabelRequest -->	<!-- Added hash to cycleon id element -->	<!-- Added URL as format -->	<xs:element name="LabelRequest">		<xs:complexType>			<xs:sequence>				<xs:element ref="auth:Authentication"  maxOccurs="1" minOccurs="1"/>				<xs:element name="CycleonId" type="CycleonIdType" maxOccurs="unbounded" minOccurs="1">					<xs:annotation>						<xs:documentation>							CycleonId of the things we're moving.						</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="PrinterLabelPath" type="xs:string" maxOccurs="1" minOccurs="0">					<xs:annotation>						<xs:documentation>The absolute system path of printed labels for current User.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="PrinterLicensePlatePath" type="xs:string" maxOccurs="1" minOccurs="0">					<xs:annotation>						<xs:documentation>The absolute system path of License Plate for current User.</xs:documentation>					</xs:annotation>				</xs:element>			</xs:sequence>			<xs:attribute name="format">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:enumeration value="SVG"/>						<xs:enumeration value="EPS"/>						<xs:enumeration value="PDF"/>						<xs:enumeration value="PNG"/>						<xs:enumeration value="JPEG"/>						<xs:enumeration value="GIF"/>						<xs:enumeration value="ZEBRA_GRF"/>						<xs:enumeration value="URL"/>						<xs:enumeration value="PRINTER"/>					</xs:restriction>				</xs:simpleType>			</xs:attribute>		</xs:complexType>	</xs:element>	<xs:complexType name="CycleonIdType">		<xs:simpleContent>			<xs:extension base="xs:string">				<xs:attribute name="hash" type="xs:string">					<xs:annotation>						<xs:documentation>The hash of the cycleon id.</xs:documentation>					</xs:annotation>				</xs:attribute>			</xs:extension>		</xs:simpleContent>	</xs:complexType>				</xs:schema>